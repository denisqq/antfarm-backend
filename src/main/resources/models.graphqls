type Query {
  farms: [Farm]
}

type Farm {
  id: String!
  colony: Colony!
}

type Colony {
  population: Int!
  antColonyPopulation: [AntColonyPopulation]
}

type AntColonyPopulation {
  id: String!
  population: Int!
  ant: Ant!
}

type Ant {
  id: String!,
  antType: AntType!,
  name: String!,
}

enum AntType {
  BIG_MOTHER, SLAVE, GUARD
}